{
  "name": "yeet-backend",
  "version": "1.0.0",
  "description": "Yeet by Yethikrishna R - Artistic Community Platform Backend API",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:integration": "jest --config jest.integration.config.js",
    "lint": "eslint src --ext ts --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts --fix",
    "format": "prettier --write 'src/**/*.ts'",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:seed": "tsx src/scripts/seed.ts",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^6.1.5",
    "compression": "^1.7.4",
    "express-rate-limit": "^6.7.0",
    "@prisma/client": "^4.14.0",
    "socket.io": "^4.6.1",
    "jsonwebtoken": "^9.0.0",
    "bcryptjs": "^2.4.3",
    "bcrypt": "^5.1.0",
    "speakeasy": "^2.0.0",
    "qrcode": "^1.5.3",
    "ethers": "^6.7.1",
    "zod": "^3.21.4",
    "dotenv": "^16.0.3",
    "nodemailer": "^6.9.1",
    "@sendgrid/mail": "^7.7.0",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.1",
    "aws-sdk": "^2.1362.0",
    "stripe": "^12.1.1",
    "redis": "^4.6.5",
    "winston": "^3.8.2",
    "express-validator": "^6.15.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-google-oauth20": "^2.0.0",
    "node-cron": "^3.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "@types/compression": "^1.7.2",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/bcrypt": "^5.0.0",
    "@types/speakeasy": "^2.0.7",
    "@types/qrcode": "^1.5.2",
    "@types/nodemailer": "^6.4.7",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.15.11",
    "@types/passport": "^1.0.12",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/node-cron": "^3.0.7",
    "typescript": "^5.0.4",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.38.0",
    "prettier": "^2.8.7",
    "tsx": "^3.12.6",
    "prisma": "^4.14.0",
    "jest": "^29.5.0",
    "@types/jest": "^29.5.1",
    "ts-jest": "^29.1.0",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.12"
  },
  "keywords": [
    "artistic-community",
    "creative-platform",
    "api",
    "backend",
    "portfolio",
    "collaboration",
    "express",
    "typescript",
    "yethikrishna"
  ],
  "author": "Yethikrishna R",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}
