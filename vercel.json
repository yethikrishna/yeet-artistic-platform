{
  "name": "yeet-platform-by-yethikrishna",
  "version": 2,
  "buildCommand": "cd yeet-website && npm run build",
  "outputDirectory": "yeet-website/dist",
  "installCommand": "cd yeet-website && npm install",
  "devCommand": "cd yeet-website && npm run dev",
  "framework": "vite",
  "redirects": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}